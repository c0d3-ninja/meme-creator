<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="@dimen/bottomSheetElevation"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bsPreviewOptionsDummy5"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >
        <HorizontalScrollView
            android:id="@+id/templatePreviewToolsHSV"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingRight="20dp"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsCreateMeme"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="10dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_creatememe_grey_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/create_meme"
                            android:layout_below="@id/bsPreviewOptionsDummy1"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsAddToFavorites"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsCreateMeme"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsAddToFavIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_star_grey_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:id="@+id/bsFavoriteText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/unfavourite"
                            android:layout_below="@id/bsAddToFavIcon"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsTemplateDownload"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsAddToFavorites"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_file_download_grey_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/save"
                            android:layout_below="@id/bsPreviewOptionsDummy2"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsTemplateEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsTemplateDownload"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_edit_grey_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/edit"
                            android:layout_below="@id/bsPreviewOptionsDummy6"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsTemplateSearchTags"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsTemplateEdit"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_search_grey_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/search_keywords"
                            android:layout_below="@id/bsPreviewOptionsDummy9"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsTemplateDelete"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsTemplateSearchTags"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_delete_red_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/delete"
                            android:layout_below="@id/bsPreviewOptionsDummy7"
                            android:layout_centerHorizontal="true"
                            android:textColor="@color/buttonColorNegative"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bsTemplateReport"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/bottomSheetListItemHeight"
                    app:layout_constraintStart_toEndOf="@id/bsTemplateDelete"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:layout_marginLeft="20dp"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/bsPreviewOptionsDummy8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_report_red_24dp"
                            android:layout_marginLeft="@dimen/headerContentStartMargin"
                            android:layout_centerHorizontal="true"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/report"
                            android:layout_below="@id/bsPreviewOptionsDummy8"
                            android:layout_centerHorizontal="true"
                            android:textColor="@color/buttonColorNegative"
                            android:layout_marginTop="5dp"
                            />
                    </RelativeLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </HorizontalScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>